alias rmpyc='find . -name "*.pyc" -delete'
alias rmswp='rm ~/.vim/tmp/*'
alias vimrc='vim $HOME/.vimrc'
alias zshrc='vim $HOME/.zshrc.local'
alias vinit='nvim $HOME/.config/nvim/init.vim'
alias vim='nvim'
alias v='nvim'
alias dots='$HOME/.dotfiles'
alias bin='$HOME/bin'
alias ta='tmux a'
alias tat='tmux a -t'
alias tk='tmux kill-session'
alias ts='tmux new-session -s'

